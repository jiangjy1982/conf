[user]
    email = jet.jiang@gotinder.com
    name = Jet Jiang
[color]
    branch = auto
    diff = auto
    interactive = auto
    pager = true
    status = auto
    ui = true
[color "status"]
    added = green
    changed = yellow
    untracked = red
[push]
    default = current
[core]
    autocrlf = input
    pager = less -FRSX
	editor = /usr/bin/vim
[alias]
    br = branch
    co = checkout
    st = status
